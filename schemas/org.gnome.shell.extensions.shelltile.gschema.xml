<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/gnome/shell/extensions/shelltile/" id="org.gnome.shell.extensions.shelltile">
    <key type="b" name="keep-group-maximized">
        <default>true</default>
        <summary>Keep the window group maximized</summary>
        <description>Enforce the group to take all the available space</description>
    </key>
    <key type="b" name="maximize-new-windows">
        <default>false</default>
        <summary>Maximize new windows</summary>
        <description>Maximizes created windows</description>
    </key>    
  </schema>
</schemalist>
